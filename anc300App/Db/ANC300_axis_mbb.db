
## TRIG_DOWN: Trigger down
record(mbbi, "$(DEV):AX$(A):TRIG_DOWN_IN") {
    field(DESC, "Trigger down")
    field(DTYP, "stream")
    field(INP, "@ANC300.protocol getStr(gettd,$(A),trigger,$(DEV)) $(PORT) 0")
    field(SCAN, "$(SCAN)")
    field(PINI, "YES")
    field(PHAS, 1)
    
    field("ZRVL", "0")
    field("ONVL", "1")
    field("TWVL", "2")
    field("THVL", "3")
    field("FRVL", "4")
    field("FVVL", "5")
    field("SXVL", "6")
    field("ZRST", "off")
    field("ONST", "1")
    field("TWST", "2")
    field("THST", "3")
    field("FRST", "4")
    field("FVST", "5")
    field("SXST", "6")

}

record(mbbo, "$(DEV):AX$(A):TRIG_DOWN_OUT") {
    field(DESC, "Trigger down")
    field(DTYP, "stream")
    field(OUT, "@ANC300.protocol setStr(settd,$(A)) $(PORT) 0")
    field(FLNK, "$(DEV):AX$(A):TRIG_DOWN_IN")

    field("ZRVL", "0")
    field("ONVL", "1")
    field("TWVL", "2")
    field("THVL", "3")
    field("FRVL", "4")
    field("FVVL", "5")
    field("SXVL", "6")
    field("ZRST", "off")
    field("ONST", "1")
    field("TWST", "2")
    field("THST", "3")
    field("FRST", "4")
    field("FVST", "5")
    field("SXST", "6")

    info(autosaveFields, "VAL")
}


